import java.util.Scanner;
class  test4pattern{
	public static void main(String[] args) 
	{
		Scanner scanner=new Scanner(System.in);
		System.out.print("enter the number of rows:");
		int n=scanner.nextInt();
		for(int i=1;i<=n;i++)
		{
			for(int j=1;j<=i;j++)
			{
				System.out.println("*");
			}
			System.out.println();
		}
	}
}

